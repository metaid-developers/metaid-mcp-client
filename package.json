{
  "name": "@metaid/metaid-mcp-client",
  "version": "1.0.0",
  "description": "Official TypeScript/JavaScript client for MetaID MCP (Model Context Protocol) Server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "metaid-mcp-client": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metaid-developers/metaid-mcp.git",
    "directory": "mcp-client-ts"
  },
  "scripts": {
    "build": "tsc",
    "build:browser": "webpack --mode production",
    "build:all": "npm run build && npm run build:browser",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "demo": "python3 -m http.server 8080",
    "setup": "./setup-browser.sh",
    "prepublish": "npm run build"
  },
  "keywords": [
    "metaid",
    "mcp",
    "model-context-protocol",
    "blockchain",
    "typescript",
    "client",
    "sse",
    "websocket"
  ],
  "author": "MetaID Team <dev@metaid.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metaid-developers/metaid-mcp/issues"
  },
  "homepage": "https://github.com/metaid-developers/metaid-mcp#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^20.10.0",
    "events": "^3.3.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

